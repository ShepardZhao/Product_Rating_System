package week3_homework;
import java.util.*;
import static java.lang.System.*;



class GameSetting{
	
	final private String[] store = {"scissors","paper","rock","lizard","Spock"};
	private  HashMap<String, Integer> Hashmap = new HashMap<String, Integer>();

	
	
	
	//constructor
	GameSetting(){
		for (int i=0;i<5;i++){
			int idx = new Random().nextInt(store.length);
			String random = store[idx];
			//out.print(random);
			Hashmap.put(random, i);
			
		}
		
	}
	
	
	
	//random check
	private String ReturnNonReplated(String string){
		
		if(this.containsKey(string)){
			return string;
		}
		else{
			return null;
		} 
	}
	
	
	//checking whether string exist
	
	private boolean containsKey(String string) {
		Object value = this.Hashmap.get(string);
		if (value == null) {
				return true;
		}
		else{
			return false;
		}
	}
	
	//condition
	
	//condition return 
	String GetResult(String value){
		return value;
	}
	
	
	//get map
	void GetMap(){
		for (Map.Entry<String, Integer> entry : Hashmap.entrySet()) {
		    String key = entry.getKey();
		    Integer value = entry.getValue();
		    out.print(key + ": " +value);
		}
	}
	
	
}


class People extends GameSetting{
	
	
	
	
}



public class Rock_Paper_Scissors_Lizard_Spock {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		GameSetting gameSetting = new GameSetting();  
		gameSetting.GetMap();
		
	}

}
