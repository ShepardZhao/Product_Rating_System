package week3_homework;
import java.util.*;
import static java.lang.System.*;



class GameSetting{
	
	final private String[] store = {"scissors","paper","rock","lizard","Spock"};
	private  HashMap<String, Integer> Hashmap = new HashMap<String, Integer>();

	
	
	
	//constructor
	GameSetting(){
		for (int i=0;i<5;i++){
			
			//out.print(random);
			if(this.ReturnNonReplated(ReturnName())){//if null
				Hashmap.put(ReturnName(), i);
			}
			else{//if exist
				if(this.ReturnLoopedValue()!=null){
					Hashmap.put(ReturnLoopedValue(), i);

				}
			}
		}
		
	}
	
	
	//while loop 
	private String ReturnLoopedValue(){
		while(ReturnNonReplated(ReturnName())){
			return ReturnName();
		}
		return null;
		
	}
	
	
	
	//generate the name
	private String ReturnName(){
		int idx = new Random().nextInt(store.length);
		String random = store[idx];
		return random;
	}
	
	
	//random check
	private boolean ReturnNonReplated(String string){
		
		if(this.isNotcontainsKey(string)){
			return true;
		}
		else{
			return false;
		} 
	}
	
	
	//checking whether string exist
	
	private boolean isNotcontainsKey(String string) {
		Object value = this.Hashmap.get(string);
		if (value == null) {
				return true;//the current value is't in Hashmap
		}
		else{
			return false;//Existed
		}
	}
	
	//condition
	
	//condition return 
	String GetResult(String value){
		return value;
	}
	
	
	//get map
	void GetMap(){
		for (Map.Entry<String, Integer> entry : Hashmap.entrySet()) {
		    String key = entry.getKey();
		    Integer value = entry.getValue();
		    out.print(key + ": " +value);
		}
	}
	
	
}


class People extends GameSetting{
	
	
	
	
}



public class Rock_Paper_Scissors_Lizard_Spock {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		GameSetting gameSetting = new GameSetting();  
		gameSetting.GetMap();
		
	}

}
